import Foundation

func solution(_ n:Int) -> Int {
    
    var haveNo3Array = [Int]()
    var i = 1
    
    while haveNo3Array.count < n {
        let j = String(i).map{String($0)}
        if i % 3 != 0 && !j.contains("3"){
            haveNo3Array.append(i)
        }
        i += 1
    }
    return haveNo3Array[n-1]
}
